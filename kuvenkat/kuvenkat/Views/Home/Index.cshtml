@model kuvenkat.Models.Empolyee
@{
    ViewBag.Title = "Employee Details";
}
<h2>Employee Details</h2>
<table style="font-family:Arial">
    <tr>
        <td>
            <b>Employee Id : </b>
        </td>
        <td>
            @Model.EmpolyeeId
        </td>
    </tr>
    <tr>
        <td>
            <b>Name : </b>
        </td>
        <td>
            @Model.name
        </td>
    </tr>
    <tr>
        <td>
            <b>Gender : </b>
        </td>
        <td>
            @Model.Gender
        </td>
    </tr>
    <tr>
        <td>
            <b>City : </b>
        </td>
        <td>
            @Model.City
        </td>
    </tr>
</table>


